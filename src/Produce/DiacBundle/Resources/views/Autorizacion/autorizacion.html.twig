                         '<td>'+$("#cboespecie option[value='"+$("#cboespecie").val()+"']").text()+'</td>'+
                         '<td>'+$("#cbofamilia option[value='"+$("#cbofamilia").val()+"']").text()+'</td>'+
                         '<td style="margin-left:auto;margin-right:auto"><a href="javascript:;" onclick="quitar_especie('+key+')"><input type="button" class="icono cus-cross-gray"></a>'+
                       '</tr>'+
                       '<input type="text" name="itemsEspecies[]" id="indice_'+$("#cboespecie").val()+'"  value="'+$("#cboespecie").val()+'">';
                 $("#especies_list tbody").append(html);
