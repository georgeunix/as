                }
                if($('input[name="itemsEspecies[]"]').length>1){
                    
                    
                    $("#radpolicultivo").attr('checked', true); 
                    
                 }else if($('input[name="itemsEspecies[]"]').length==1){
                    
                    $("#radmonocultivo").attr('checked', true); 
                    
