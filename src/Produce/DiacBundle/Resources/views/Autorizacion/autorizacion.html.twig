            function quitar_especie(id_remove,id_grupo){                                                                   

                $("#"+id_remove+"").remove();
                myspecies.splice(id_remove);
                
                if(myspeciesgrupo[id_grupo]==1){
                    myspeciesgrupo[id_grupo]=0;
                     $("#grupo_"+id_grupo+"").remove();
                     //myspeciesgrupo.splice(id_grupo,1);
                }
                else{
                   myspeciesgrupo[id_grupo]=myspeciesgrupo[id_grupo]-1; 
                }
                if($('input[name="itemsEspecies[]"]').length>1){
                    
                    
                    $("#radpolicultivo").attr('checked', true); 
                    
                 }else if($('input[name="itemsEspecies[]"]').length==1){
                    
                    $("#radmonocultivo").attr('checked', true); 
                    
                 }else{
                     $("#radpolicultivo").attr('checked', false);
                     $("#radmonocultivo").attr('checked', false); 
                 }
                
            }
            
            function agregar_especie(){  
                
                for (i=0;i<myspecies.length;i++){
                     if(myspecies[i]==$("#cboespecie").val()){
                         alert('Ya ingreso la especie selccionada');
                         return false;
                     }
                }
                 msg='';
                 if($("#cbofamilia").val()==0){
                     msg+="-Seleccione Familia\n";
                 }
                 if($("#cboespecie").val()==0){
                     msg+="-Seleccione Especie\n";
                 }
                  if(msg!=''){
                      alert(msg);
                      return false;
                  }
                 key_grupo=$("#txtgrupoespecieid").val(); 
                 
                 html='<tr id='+key+'>'+
                         '<td>'+$("#cboespecie option[value='"+$("#cboespecie").val()+"']").text()+'</td>'+
                         '<td>'+$("#cbofamilia option[value='"+$("#cbofamilia").val()+"']").text()+'</td>'+
                         '<td style=""><a href="javascript:;" onclick="quitar_especie('+key+','+key_grupo+')"><span class="btn"><i class="cus cus-delete"></i></span></a>'+
                         '<input type="hidden" name="itemsEspecies[]" id="indice_'+$("#cboespecie").val()+'"  value="'+$("#cboespecie").val()+'"></td>'+     
