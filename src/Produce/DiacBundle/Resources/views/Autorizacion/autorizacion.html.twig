             function validar_upload_file_sucursal(){
                   
                    var archivos = document.getElementById("archivosucursal");
                    var archivo = archivos.files; 
                    var data = new FormData();
                    
                    for(i=0; i<archivo.length; i++){
                        data.append('archivo'+i,archivo[i]);
                    }
                    //data.append('nombre',15); 
                    
                    $.ajax({
                        url:'{{path('validarUploadFile')}}', 
                        type:'POST', 
                        contentType:false, 
                        data:data, 
                        processData:false, 
                        cache:false,
                        
                        success: function(msg) {
                            if(msg==2){
                               alert('Formato No Válido, Permitidos .PDF');
                               $("#archivoSucursal").val('');
                           }else if(msg==3){
                               alert('Tamaño Maximo Permitido 2MB');
                               $("#archivoSucursal").val('');
                           }else{
                               $("#namearchivosucursal").val(msg);
                           }
                            
                        },
                        error: function(msg) {
                           alert('Error en archivo');
                        }
                        
                    });
                }
             
