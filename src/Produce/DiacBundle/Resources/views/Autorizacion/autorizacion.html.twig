            function quitar_especie(id_remove){

                $("#"+id_remove+"").remove();
                myspecies.splice(id_remove);
            }
            
            var myspecies = new Array();
            var key=0;
            function agregar_especie(){                   
                for (i=0;i<myspecies.length;i++){
                     if(myspecies[i]==$("#cboespecie").val()){
                         alert('Ya ingreso la especie selccionada');
                         return false;
                     }
                }
                 msg='';
                 if($("#cbofamilia").val()==0){
                     msg+="-Seleccione Familia\n";
                 }
                 if($("#cboespecie").val()==0){
                     msg+="-Seleccione Especie\n";
                 }
                  if(msg!=''){
                      alert(msg);
                      return false;
                  }
